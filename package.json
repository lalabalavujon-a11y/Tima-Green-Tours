{
  "name": "tima-green-tours",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "verify:node": "node scripts/check-node-alignment.mjs",
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "npm run verify:node && npm run build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "debug:start": "next lint",
    "debug:fix": "next lint --fix",
    "debug:monitor": "super-debug monitor",
    "cf:purge:app": "node scripts/purge-cloudflare.mjs app.timagreentours.com",
    "cf:purge:all": "CF_PURGE_ALL=1 node scripts/purge-cloudflare.mjs",
    "assistants:sync": "tsx scripts/sync-assistants.ts",
    "gen:client": "openapi-typescript apps/api/src/docs/openapi-simple.yaml -o lib/api-types.ts"
  },
  "dependencies": {
    "@langchain/core": "^0.3.75",
    "@langchain/openai": "^0.6.11",
    "clsx": "2.1.1",
    "next": "14.2.5",
    "openai": "^4.59.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "3.4.0",
    "stripe": "^18.5.0",
    "yaml": "^2.6.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.7",
    "tsx": "^4.19.0",
    "typescript": "5.9.2",
    "openapi-typescript": "^6.7.4"
  }
}
